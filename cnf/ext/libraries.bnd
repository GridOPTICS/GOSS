# Config Admin
#
configadmin: ${repo;org.apache.felix.configadmin;[1.8.0,1.8.1);HIGHEST}
configadmin-buildpath: ${configadmin};version=file
configadmin-runpath: ${configadmin};version=file	
	
# Amdatu Components
